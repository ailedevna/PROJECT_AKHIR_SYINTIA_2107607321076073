<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/pink"
    tools:context=".pejuang_activity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/pejuang_txt1"
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="25dp"
            android:background="@drawable/background_header"
            android:fontFamily="@font/lemon_days"
            android:gravity="center_horizontal"
            android:text="PEJUANG DAN TOKOH POLITIK"
            android:textColor="@color/white"
            android:textSize="25dp" />

        <ImageView
            android:id="@+id/pejuang_gambar1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/pejuang_txt1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:background="@drawable/button_shape"
            android:src="@drawable/abdulhlim"
            tools:ignore="SpeakableTextPresentCheck"/>

        <Button
            android:id="@+id/pejuang_txt2"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/pejuang_gambar1"
            android:text="ABDUL HALIM"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:backgroundTint="@color/merahjambu"
            android:fontFamily="sans-serif-condensed-medium"
